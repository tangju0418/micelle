{
    "manifest_version": 2,

    "name": "Micelle",
    "description": "A extension to manage your bookmarks.",
    "version": "1.0",

    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}
